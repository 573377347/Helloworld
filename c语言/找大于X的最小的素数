#include <stdio.h>
#include <stdlib.h>

int f(int n)
{
    int m,i;
    if(n<2)
     return 2;
     else
     {
         while(1)
         {
             n++;
             for(i=2;i<n;i++)
             {
                 if(n%i==0)
                    break;
             }
             if(i==n) break;
         }
         return n;
     }
}
